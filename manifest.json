{
  "name": "dyStore",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "version": "1.0",
  "description": "dyStore devTools",
  "browser_action": {
    "default_icon": "img/logo.png"
  },
  "devtools_page": "devtools.html",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/hook.js"
      ],
      "run_at": "document_start"
    }
  ]
}

